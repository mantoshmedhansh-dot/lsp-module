{
  "name": "oms",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:web": "npm run prisma:generate && cd apps/web && npm run build",
    "lint": "turbo run lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prisma:generate": "cd packages/database && npx prisma generate",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:push:local": "cd packages/database && npx prisma db push",
    "db:migrate": "cd packages/database && npx prisma migrate dev",
    "db:studio": "cd packages/database && npx prisma studio",
    "db:seed": "cd packages/database && npm run db:seed",
    "db:reset": "cd packages/database && npx prisma migrate reset --force",
    "docker:up": "docker compose up -d postgres",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f postgres",
    "docker:pgadmin": "docker compose --profile tools up -d",
    "local:setup": "./scripts/start-local.sh",
    "vercel-build": "npm run prisma:generate && cd apps/web && npm run build",
    "start": "cd apps/web && npm start",
    "deploy": "./scripts/deploy-all.sh",
    "deploy:vercel": "npm run vercel-build && npx vercel --prod --yes",
    "deploy:render": "git push singh master:main",
    "push:all": "git push origin master && git push singh master:main",
    "validate": "npm run prisma:generate && npm run build:web"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.14",
    "@vitest/coverage-v8": "^4.0.17",
    "embedded-postgres": "^18.1.0-beta.15",
    "pg-mem": "^3.0.5",
    "turbo": "^2.0.0",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
